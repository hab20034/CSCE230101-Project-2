`timescale 1ns / 1ps

module asciiRom(
    input clk, 
	input wire [10:0] add,
	output reg [7:0] data
    );
    
    (* rom_style = "block" *)

	reg [10:0] addReg;
	
	always @(posedge clk)
		addReg <= add;
    always @*
		case(addReg)
		
			// (nul)
			11'h000: data = 8'b00000000;	
			11'h001: data = 8'b00000000;	
			11'h002: data = 8'b00000000;	
